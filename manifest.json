{
  "manifest_version": 3,
  "name": "Ultima",
  "version": "1.1.0",
  "description": "一个美观实用的新标签页扩展，包含快捷方式、壁纸、天气、待办事项、笔记、书签等功能",
  "permissions": [
    "storage",
    "bookmarks",
    "topSites",
    "tabs"
  ],
  "optional_permissions": [
    "history"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/shadcn.jpg",
    "48": "icons/shadcn.jpg",
    "128": "icons/shadcn.jpg"
  }
}
